<h1 class="center">Sell or Rent your Property</h1>
<section id="form-container">
    <div id="forms-box">
        <form action="">
            <div class="form-group">
                <label for="Name">Name:-</label>
                <input type="text" name="Name" id="Name" placeholder="Enter Your Name">
            </div>
            <div class="form-group">
                <label for="Name">Email:-</label>
                <input type="text" name="email" id="email" placeholder="Enter Your Email">
            </div>
            <div class="form-group">
                <label for="contact">Contact Number:-</label>
                <input type="text" name="contact" id="contact" placeholder="Enter Your Contact Number">
            </div>
            <div class="form-group">
                <label for="addres">Address:-</label>
                <input type="text" name="addres" id="addres" placeholder="Enter Your Addres">
            </div>
            <div class="form-group">
                <label for="Personal Details">Personal Details:-</label>

                <select class="form-group1">
                    <option value="a">Owner</option>
                    <option value="b">Agent</option>
                    <option value="c">Builder</option>
                </select>
            </div>
            <div class="form-group">
                <label for="Property Details">What are you looking for:-</label>

                <select class="form-group1">
                    <option value="a">sale</option>
                    <option value="b">Rent</option>
                    <option value="c">Pg/Hostel</option>
                </select>
            </div>
            <div class="form-group">

                <label for="Property Type">Property Type:- </label>

                <select class="form-group1">
                    <option value="All Residential">Flat</option>
                    <!-- <option value="b">Flat</option> -->
                    <option value="c">apartment</option>
                    <option value="d">Villa</option>
                    <option value="e">Plot</option>
                    <option value="f">Farm House</option>
                </select>

            </div>
            <div class="form-group">
                <label for="Pin code">Poroperty Pin Code:-</label>
                <input type="text" name="addres" id="addres" placeholder="Ener Property Pin Code">
            </div>


            <div class="form-group">
                <label for="message">Message:-</label>
                <textarea name="Message" id="message" cols="30" rows="10"></textarea>
            </div>
            <div class="form-group">
                <center> <!-- <button class="btn"> Submit now</button> -->
                <div id="liveAlertPlaceholder"></div>
                <button type="button" class="btn" id="liveAlertBtn">Submit now</button></center>
            </div>


        </form>

    </div>


